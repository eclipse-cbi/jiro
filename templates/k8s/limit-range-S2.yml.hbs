apiVersion: "v1"
kind: "LimitRange"
metadata:
  {{> common-metadata }}
  name: "jenkins-instance-limit-range"
spec:
  limits:
    - type: "Pod"
      {{> limit-range-default-pod }}
      max:
        cpu: 6200m
        memory: 16640Mi
    - type: "Container"
      {{> limit-range-default-container }}
      max:
        cpu: 6000m
        memory: 16384Mi