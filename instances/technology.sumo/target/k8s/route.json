{
   "apiVersion": "route.openshift.io/v1",
   "kind": "Route",
   "metadata": {
      "annotations": {
         "haproxy.router.openshift.io/timeout": "60s"
      },
      "labels": {
         "org.eclipse.cbi.jiro/project.fullName": "technology.sumo",
         "org.eclipse.cbi.jiro/project.shortname": "sumo"
      },
      "name": "sumo",
      "namespace": "sumo"
   },
   "spec": {
      "host": "ci.eclipse.org",
      "path": "/sumo",
      "port": {
         "targetPort": "http"
      },
      "tls": {
         "insecureEdgeTerminationPolicy": "Redirect",
         "termination": "edge"
      },
      "to": {
         "kind": "Service",
         "name": "jenkins-ui",
         "weight": 100
      }
   }
}
